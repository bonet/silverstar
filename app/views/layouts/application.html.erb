<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  
  <%= javascript_include_tag "application" %>
  <%= render "layouts/stylesheets" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%= render "layouts/header" %>
		<section class="round">
			<%= yield %>
		</section>
		<%= render "layouts/footer" %>
		<%= debug(params) if Rails.env.development? %>
	</div>
</body>
</html>
